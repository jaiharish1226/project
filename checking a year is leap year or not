/*to check if a year is leap year or not*/
#include <stdio.h>
int main()
/*opening brackets*/
{
  
	int y;
	/*taking an integer value for the year*/
	printf("enter year :");
	scanf("%d", &y);
	/*checking if a value is leap year or not*/
	if(y%4==0 && y%100!=0)
	printf("%d is leap year", y);
	/*printing the year is leap year or not*/
	else if(y%400==0)
	printf("%d is a leap year", y);
		/*printing the year is leap year or not*/
	else if(y%100)
	printf("%d is not a leap year",y);
		/*printing the year is leap year or not*/
	else
	printf("%d is not a leap year",y);
		/*printing the year is leap year or not*/
	return 0;
}
/*closing brackets*/




